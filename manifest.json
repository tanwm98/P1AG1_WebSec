{
  "manifest_version": 3,
  "name": "ExMM",
  "version": "1.0",
  "description": "Exploit Mitigation Monitor",
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "windows",
    "scripting",
    "webRequest",
    "webNavigation"
  ],
  "host_permissions": [
    "https://dns.google/*",
    "<all_urls>"
  ],
  "web_accessible_resources": [{
    "resources": [
      "tab1.html",
      "tab2.html",
      "tab3.html",
      "tab4.html",
      "tab5.html",
      "script/*",
      "style/*"
    ],
    "matches": ["<all_urls>"]
  }],
  "action": {
    "default_popup": "main.html",
    "default_icon": {
      "16": "media/icon16.png",
      "48": "media/icon48.png",
      "128": "media/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script/urlParser.js"]
    }
  ],
  "background": {
    "service_worker": "script/background.js"
  }
}