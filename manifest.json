{
  "manifest_version": 3,
  "name": "eXMM",
  "version": "1.0",
  "description": "Exploit Mitigation Monitor",
  "permissions": ["storage", "tabs"],
  "host_permissions": ["https://dns.google/*"],
  "action": {
    "default_popup": "main.html",
    "default_icon": {
      "16": "media/icon16.png",
      "48": "media/icon48.png",
      "128": "media/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches":
      ["<all_urls>"],
      "js": ["script/Parser.js", "script/urlParser.js"]
    }
  ],
  "background": {
    "service_worker": "script/background.js"
  }
}