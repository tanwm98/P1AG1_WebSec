{
  "manifest_version": 3,
  "name": "Simple Extension",
  "version": "1.0",
  "description": "A simple browser extension example",
  "permissions": ["storage"],
  "action": {
    "default_popup": "main.html",
    "default_icon": {
      "16": "media/icon16.png",
      "48": "media/icon48.png",
      "128": "media/icon128.png"
    }
  }
}
